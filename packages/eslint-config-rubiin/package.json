{
  "name": "@rubiin/eslint-config",
  "type": "module",
  "version": "1.8.12",
  "packageManager": "pnpm@8.8.0",
  "description": "Anthony's ESLint config",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com> (https://github.com/rubiin/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/rubiin",
  "homepage": "https://github.com/rubiin/eslint-config",
  "keywords": [
    "eslint-config",
    "rubiin",
    "eslint",
    "eslint-config-rubiin",
    "eslint-config-typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "stub": "tsup src/index.ts --format esm",
    "dev": "tsup src/index.ts --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "pnpm run stub && eslint .",
    "lint:fix": "pnpm run stub && eslint . --fix",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@arthurgeron/eslint-plugin-react-usememo": "^2.0.1",
    "eslint-plugin-rubiin": "workspace:*",
    "@stylistic/eslint-plugin": "0.0.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@typescript-eslint/utils": "^6.7.4",
    "eslint-config-flat-gitignore": "^0.1.0",
    "eslint-define-config": "^1.24.0",
    "eslint-plugin-antfu": "^1.0.0-beta.12",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.2",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-yml": "^1.10.0",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.3.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.8",
    "@stylistic/eslint-plugin-migrate": "^0.0.6",
    "@types/eslint": "^8.44.3",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.3",
    "@typescript-eslint/rule-tester": "^6.7.4",
    "@typescript-eslint/utils": "^6.7.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "eslint-flat-config-viewer": "^0.1.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "lint-staged": "^14.0.1",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "sucrase": "^3.34.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
